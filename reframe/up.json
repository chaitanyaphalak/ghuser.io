{
  "name": "ghuser",
  "profile": "default",
  "_comment": "Only us-east-1 works properly, see https://github.com/apex/up/issues/501#issuecomment-361103668",
  "regions": [
    "us-east-1"
  ],
  "lambda": {
    "memory": 256
  },
  "stages": {
    "staging": {
      "domain": "www.ghuser.io"
    },
    "production": {
      "domain": "ghuser.io"
    }
  },
  "environment": {
    "comment_": "for OAuth and profile request creation:",
    "AWS_SQS_QUEUE_URL": "$AWS_SQS_QUEUE_URL",
    "COOKIE_ENCRYPTION_PASSWORD": "$COOKIE_ENCRYPTION_PASSWORD",
    "GITHUB_CLIENT_ID": "$GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET": "$GITHUB_CLIENT_SECRET",
    "SENTRY_DNS": "$SENTRY_DNS",

    "comment__": "Underscore at the end is a temporary fix for the build. Without it Up fails with https://github.com/apex/apex/issues/829",
    "AWS_ACCESS_KEY_ID_": "$AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY_": "$AWS_SECRET_ACCESS_KEY"
  }
}
